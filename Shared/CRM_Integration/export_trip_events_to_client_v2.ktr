    <connection>CRSINC</connection>
    <sql>select t.id trip_id, p.username, p.client_employee_id, p.first_name, p.last_name, &#xd;&#xa;trim&#x28; both &#x27; &#x27; from split_part&#x28;p.email, &#x27;,&#x27;, 1&#x29;&#x29; email, &#xd;&#xa;p.time_zone_name, t.trip_day, &#xd;&#xa;CAST&#x28;from_lm.detected_departure_timestamp as TIMESTAMPTZ&#x29; AT TIME ZONE p.time_zone_name as departure_time, &#xd;&#xa;from_loc.full_address as starting_address, from_pl.name as starting_name, &#xd;&#xa;to_loc.full_address as ending_address, &#xd;&#xa;&#x27;&#x28;M&#x29; - &#x27; &#x7c;&#x7c; case when to_pl.name is null then to_loc.full_address else to_pl.name end as ending_name, &#xd;&#xa;case when to_lm.gps_verified is false then to_char&#x28;&#x28;&#x28;t.trip_day &#x2b; interval &#x27;18 hours&#x27;&#x29;&#x3a;&#x3a;timestamp AT TIME ZONE p.time_zone_name&#x29; AT TIME ZONE &#x27;UTC&#x27;, &#x27;YYYY-MM-DD&#x22;T&#x22;HH24&#x3a;MI&#x3a;SS&#x22;Z&#x22;&#x27;&#x29;&#xd;&#xa;     else to_char&#x28;to_lm.stated_timestamp AT TIME ZONE &#x27;UTC&#x27;, &#x27;YYYY-MM-DD&#x22;T&#x22;HH24&#x3a;MI&#x3a;SS&#x22;Z&#x22;&#x27;&#x29; end as arrival_time,&#xd;&#xa;CAST&#x28;to_lm.detected_departure_timestamp as TIMESTAMPTZ&#x29; AT TIME ZONE p.time_zone_name as stop_end_time, &#xd;&#xa;&#x28;to_lm.detected_departure_timestamp - to_lm.stated_timestamp&#x29; as stop_duration, &#xd;&#xa;&#x28;to_lm.stated_timestamp - from_lm.stated_timestamp&#x29; as trip_duration, &#xd;&#xa;t.custom_field_values -&#x3e; &#x27;purpose&#x27; purpose, &#xd;&#xa;case when t.custom_field_values-&#x3e;&#x27;notes&#x27; &#x3d; &#x27;&#x27; then &#x27;None&#x27; else t.custom_field_values-&#x3e;&#x27;notes&#x27; end as notes,&#xd;&#xa;t.distance as distance, &#xd;&#xa;case when to_lm.gps_verified then &#x27;GPS&#x27; else &#x27;Manual&#x27; end gps_verified,  &#xd;&#xa;case &#xd;&#xa;when to_lm.data_entry_source &#x3d; 1 then &#x27;location_logger&#x27; &#xd;&#xa;when to_lm.data_entry_source &#x3d; 2 then &#x27;browser&#x27; &#xd;&#xa;when to_lm.data_entry_source &#x3d; 3 then &#x27;mileage_tracker&#x27; end as entry_source,&#xd;&#xa;to_cloc.location_tags-&#x3e;&#x27;salesforceObjectType&#x27; salesforceObjectType,&#xd;&#xa;to_cloc.location_tags-&#x3e;&#x27;salesforceObjectId&#x27; salesforceObjectId,&#xd;&#xa;to_cloc.location_tags-&#x3e;&#x27;salesforceAccountId&#x27; salesforceAccountId&#xd;&#xa;from trip t &#xd;&#xa;join location_mark from_lm on t.from_location &#x3d; from_lm.id &#xd;&#xa;join person_location from_pl on from_lm.stated_location &#x3d; from_pl.id &#xd;&#xa;join location from_loc on from_pl.location &#x3d; from_loc.id &#xd;&#xa;join location_mark to_lm on t.to_location &#x3d; to_lm.id &#xd;&#xa;join person_location to_pl on to_lm.stated_location &#x3d; to_pl.id &#xd;&#xa;join location to_loc on to_pl.location &#x3d; to_loc.id &#xd;&#xa;join person p on p.id &#x3d; t.person &#xd;&#xa;left outer join client_location from_cloc on from_pl.client_location &#x3d; from_cloc.id&#xd;&#xa;left outer join client_location to_cloc on to_pl.client_location &#x3d; to_cloc.id&#xd;&#xa;where t.last_modified_date &#x3e;&#x3d; date_trunc&#x28;&#x27;day&#x27;, now&#x28;&#x29;&#x29;&#x3a;&#x3a;timestamp - &#x27;3 days&#x27;&#x3a;&#x3a;interval&#xd;&#xa;and p.org_unit_id &#x3d; &#x24;&#x7b;client_id&#x7d;&#xd;&#xa;order by p.email&#x3b;</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
